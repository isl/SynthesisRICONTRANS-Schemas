<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:bibtex="http://bibtexml.sf.net/" elementFormDefault="qualified" attributeFormDefault="qualified">
	<xsd:include schemaLocation="Types.xsd"/>
	<xsd:element name="Bibliography">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="IdentificationNumber" type="identificationNumber"/>
				<xsd:element ref="Citation" maxOccurs="unbounded"/>
				<xsd:element name="KindOfBibliography" type="term"/>
				<xsd:element name="Publisher" type="term" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element name="Title" type="xsd:string" minOccurs="0"/>
				<xsd:element name="Author" type="term" maxOccurs="unbounded"/>
				<xsd:element name="Supervision" type="term" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element name="PublicationDate" type="xsd:string" minOccurs="0"/>
				<xsd:element name="PlaceOfPublication" type="term" minOccurs="0"/>
				<xsd:element name="ConferenceTitle" type="xsd:string" minOccurs="0"/>
				<xsd:element name="PlaceOfConference" type="term" minOccurs="0"/>
				<xsd:element name="BookTitle" type="xsd:string" minOccurs="0"/>
				<xsd:element name="Organisation" type="term" minOccurs="0"/>
				<xsd:element name="UserRights" type="term" minOccurs="0"/>
				<xsd:element name="ISBN" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element name="ISSN" type="xsd:string" minOccurs="0"/>
				<xsd:element name="URL" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element name="Annotations" type="xsd:string" minOccurs="0"/>
				<xsd:element name="NameOfJournalOrPeriodical" type="term" minOccurs="0"/>
				<xsd:element name="AbbreviationOfMagazine" type="term"/>
				<xsd:element name="VolumeNumber" type="xsd:string" minOccurs="0"/>
				<xsd:element name="IssueNumber" type="xsd:string" minOccurs="0"/>
				<xsd:element name="SeriesTitle" type="term" minOccurs="0"/>
				<xsd:element name="SeriesNumber" type="xsd:string" minOccurs="0"/>
				<xsd:element name="AbbreviationOfSeries" type="term"/>
				<xsd:element name="Language" type="term"/>
				<xsd:element name="Link" type="broswe_all"/>
				<xsd:element ref="admin" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
