<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:complexType name="thesaurus">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="sps_id" type="xs:string" use="required"/>
				<xs:attribute name="sps_facet" type="xs:string" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="multiple_files">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="sps_multiple" type="xs:string" use="required"/>
				<xs:attribute name="sps_browse" type="xs:string" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:element name="ReferencedInformation">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Date" maxOccurs="unbounded">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="time_span">
								<xs:attribute name="x" type="xs:string" use="required"/>
								<xs:attribute name="y" type="xs:string" use="required"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="Place" type="term" maxOccurs="unbounded"/>
				<xs:element name="Person" type="term" maxOccurs="unbounded"/>
				<xs:element name="Object" type="term" maxOccurs="unbounded"/>
				<xs:element name="Organization" type="term" maxOccurs="unbounded"/>
				<xs:element name="Event" type="term" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Citation">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Style" type="term"/>
				<xs:element name="Text" type="richText"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="NameNativeLanguage">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Name" type="xs:string"/>
				<xs:element name="Language" type="term"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="event">
		<xs:sequence>
			<xs:element name="Appellation" type="term" maxOccurs="unbounded"/>
			<xs:element name="Type" type="term" maxOccurs="unbounded"/>
			<xs:element name="Chronology" type="chronology"/>
			<xs:element name="Location" type="location" maxOccurs="unbounded"/>
			<xs:element name="PersonInvolved" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Person" type="link"/>
						<xs:element name="Roles" type="term" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="OrganisationInvolved" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Organisation" type="link"/>
						<xs:element name="Roles" type="term" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ObjectInvolved" type="object" maxOccurs="unbounded"/>
			<xs:element name="Description"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="admin">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="id" type="xs:string"/>
				<xs:element name="uri_id" type="xs:string"/>
				<xs:element name="lang" type="xs:string"/>
				<xs:element name="organization" type="xs:string"/>
				<xs:element name="creator" type="xs:string"/>
				<xs:element name="saved" type="xs:string"/>
				<xs:element name="locked" type="xs:string"/>
				<xs:element name="status" type="xs:string"/>
				<xs:element name="versions">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="numOfVersions" minOccurs="0"/>
							<xs:element name="webVersion" minOccurs="0"/>
							<xs:element name="versionId"/>
							<xs:element name="versionUser"/>
							<xs:element name="versionDate"/>
							<xs:element name="comment" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="type" minOccurs="0"/>
				<xs:choice minOccurs="1" maxOccurs="unbounded">
					<xs:element name="read" type="xs:string"/>
					<xs:element name="write" type="xs:string" maxOccurs="unbounded"/>
					<xs:element name="info" type="xs:string" minOccurs="0"/>
					<xs:element name="translationFrom" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="entityType" minOccurs="0"/>
					<xs:element name="publicationEnable" type="xs:string" minOccurs="0"/>
					<xs:element name="schemaVersion" minOccurs="0"/>
					<xs:element name="copyFrom" type="xs:string" minOccurs="0"/>
					<xs:element name="imported" type="xs:string" minOccurs="0"/>
					<xs:element name="createdAt" minOccurs="0"/>
					<xs:element name="lastModified" minOccurs="0"/>
					<xs:element name="refs" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ref" minOccurs="1" maxOccurs="unbounded">
									<xs:complexType>
										<xs:attribute name="sps_type" use="required"/>
										<xs:attribute name="sps_id" use="required"/>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="refs_by" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ref_by" minOccurs="0" maxOccurs="unbounded">
									<xs:complexType>
										<xs:attribute name="sps_type" use="required"/>
										<xs:attribute name="sps_id" use="required"/>
										<xs:attribute name="isUnpublished"/>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="identificationNumber">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="sps_fixed" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="codeNumber">
		<xs:sequence>
			<xs:element name="Value" type="xs:string"/>
			<xs:element name="Category" type="term" maxOccurs="unbounded"/>
			<xs:element name="AttributionDate" type="date"/>
			<xs:element name="ExpiryDate" type="date"/>
			<xs:element name="AttributedBy" type="link" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="code">
		<xs:sequence>
			<xs:element name="CodeValue">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string"/>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Category" type="term" maxOccurs="unbounded"/>
			<xs:element name="AttributedBy" maxOccurs="unbounded">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="link"/>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="AttributionDate" type="date"/>
			<xs:element name="ExpiryDate" type="date"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="codeLockedFields">
		<xs:sequence>
			<xs:element name="CodeValue">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="sps_fixed" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Category" maxOccurs="1">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="sps_fixed" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="AttributedBy" maxOccurs="1">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="sps_fixed" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="AttributionDate">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="sps_fixed" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="codeURI">
		<xs:sequence>
			<xs:element name="CodeValue">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="sps_fixed" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Category" type="term" maxOccurs="unbounded"/>
			<xs:element name="AttributedBy" maxOccurs="unbounded">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="link"/>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="AttributionDate" type="date"/>
			<xs:element name="ExpiryDate" type="date"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="location">
		<xs:sequence>
			<xs:element name="GeopoliticalHierarchy" type="term"/>
			<xs:element name="Link" type="link"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="contactInfo">
		<xs:sequence>
			<!--	<xs:element name="Location" type="location" maxOccurs="unbounded"/>  -->
			<xs:element name="Address" type="xs:string" minOccurs="0"/>
			<xs:element name="Telephone" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Fax" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Email" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="WebAddress" type="url"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="richText">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="sps_html" type="xs:string" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="thumb">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="sps_browse" type="xs:string" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="broswe_all">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="sps_browse" type="xs:string" use="required" fixed="all"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="term">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="sps_id" type="xs:string" use="required"/>
				<xs:attribute name="sps_vocabulary" type="xs:string" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="link">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="sps_id" type="xs:string" use="required"/>
				<xs:attribute name="sps_type" type="xs:string" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:element name="Date" type="date"/>
	<xs:complexType name="date">
		<xs:sequence>
			<xs:element name="DateValue">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="time_span">
							<xs:attribute name="x" type="xs:string" use="required"/>
							<xs:attribute name="y" type="xs:string" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="dateFromTo">
		<xs:sequence>
			<xs:element name="From">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="time_span">
							<xs:attribute name="x" type="xs:string" use="required"/>
							<xs:attribute name="y" type="xs:string" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="To">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="time_span">
							<xs:attribute name="x" type="xs:string" use="required"/>
							<xs:attribute name="y" type="xs:string" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TimePeriod">
		<xs:sequence>
			<xs:element name="DateValue">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="time_span">
							<xs:attribute name="x" type="xs:string" use="required"/>
							<xs:attribute name="y" type="xs:string" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--
	<xs:complexType name="date">
		<xs:simpleContent>
			<xs:extension base="time_span">
				<xs:attribute name="x" type="xs:string" use="optional"/>
				<xs:attribute name="y" type="xs:string" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	-->
	<xs:element name="Chronology">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="chronology"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="chronology">
		<xs:sequence>
			<xs:element name="AtSomeTimeWithin" type="date"/>
			<xs:element name="InTheCourseOf" type="date"/>
			<xs:element name="FromTo" type="dateFromTo"/>
			<xs:element name="EventRelated" type="term" maxOccurs="unbounded"/>
			<xs:element name="CulturalPeriod" type="term" maxOccurs="unbounded"/>
			<xs:element name="Justification" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="zip">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="sps_browse" type="xs:string" use="required" fixed="zip"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="externalLink">
		<xs:simpleContent>
			<xs:extension base="url">
				<xs:attribute name="sps_url" type="xs:string" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="url">
		<xs:restriction base="xs:string">
			<xs:pattern value="[hH][tT]{2}[pP][sS]?://.*"/>
			<xs:pattern value="\s*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="audio">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="sps_browse" type="xs:string" use="required" fixed="audio"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="video">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="sps_browse" type="xs:string" use="required" fixed="video"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="value">
		<xs:sequence>
			<xs:element name="From" type="xs:string"/>
			<xs:element name="To" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="object">
		<xs:sequence>
			<xs:element name="CodeNumber" type="link"/>
			<xs:sequence>
				<xs:element name="Appellation" type="xs:string"/>
				<xs:element ref="Kind" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
		<xs:attribute name="sps_link" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:element name="Kind" type="ObjectType"/>
	<xs:complexType name="ObjectType">
		<xs:sequence>
			<xs:element name="FunctionType" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="Appellation"/>
						<xs:element name="Thesaurus" type="term"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="MorphologyType" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="Appellation"/>
						<xs:element name="Thesaurus" type="term"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ConstructionType" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="Appellation"/>
						<xs:element name="Thesaurus" type="term"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="LocationRelated" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="Appellation"/>
						<xs:element name="Thesaurus" type="term"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="CompositionRelated" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="Appellation"/>
						<xs:element name="Thesaurus" type="term"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="StructuralSystem" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="Appellation"/>
						<xs:element name="Thesaurus" type="term"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="CompositionPrinciples" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="Appellation"/>
						<xs:element name="Thesaurus" type="term"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="ArchitecturalTypeRelated" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ArchitecturalTypeRelated">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ArchitecturalType" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Appellation"/>
							<xs:element name="Thesaurus" type="term"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ArchitecturalTypeDescription" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Appellation" type="term"/>
	<xs:element name="ObjectKind" type="thesaurus"/>
	<xs:complexType name="KindOfMobileObject">
		<xs:sequence>
			<xs:element name="FunctionType" type="thesaurus" maxOccurs="unbounded"/>
			<xs:element name="MorphologyType" type="thesaurus" maxOccurs="unbounded"/>
			<xs:element name="ConstructionType" type="thesaurus" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Place" type="place"/>
	<xs:complexType name="place">
		<xs:sequence>
			<xs:element name="Location" type="link" maxOccurs="unbounded"/>
			<xs:element name="Address" type="xs:string"/>
			<xs:element name="Coordinates">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Values">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Χ1" type="xs:string"/>
									<xs:element name="Χ2" type="xs:string"/>
									<xs:element name="Υ1" type="xs:string"/>
									<xs:element name="Υ2" type="xs:string"/>
									<xs:element name="Ζ1" type="xs:string"/>
									<xs:element name="Ζ2" type="xs:string"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="MeasurementMethod" type="term"/>
						<xs:element name="MeasurementAccuracy" type="xs:string"/>
						<xs:element name="GeodeticCoordinatesSystem" type="term"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="Image"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Object" type="object"/>
	<xs:element name="ObjectInvolved" type="object"/>
	<xs:element name="Image">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Link" type="link"/>
				<xs:element name="Comment"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MobileObject" type="mobileObject"/>
	<xs:complexType name="mobileObject">
		<xs:sequence>
			<xs:element name="CodeNumber" type="link"/>
			<xs:sequence>
				<xs:element name="Appellation" type="xs:string"/>
				<xs:element ref="ObjectKind" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
		<xs:attribute name="sps_link" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:element name="ObjectInvolvedForMobileObject" type="mobileObject"/>
	<xs:simpleType name="time_span">
